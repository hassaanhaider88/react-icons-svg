{
  "manifest_version": 3,
  "name": "React Icons SVG Grabber",
  "version": "1.0",
  "description": "Grab and copy SVG icons directly from the React Icons website.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://react-icons.github.io/*"],
  "content_scripts": [
    {
      "matches": ["https://react-icons.github.io/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/SVGCopire.png",
      "32": "icons/SVGCopire.png",
      "128": "icons/SVGCopire.png"
    }
  },
  "icons": {
    "16": "icons/SVGCopire.png",
    "32": "icons/SVGCopire.png",
    "128": "icons/SVGCopire.png"
  }
}
